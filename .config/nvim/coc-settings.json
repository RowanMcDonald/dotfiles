{
  "list.maxHeight": 15,
  "coc.preferences.formatOnType": false,
  "suggest.noselect": false,
  "coc.source.emoji.triggerCharacters": [":"],
  "coc.source.emoji.filetypes": [
    "markdown",
    "text",
    "gitcommit"
  ],
  "coc.source.word.filetypes": ["markdown"],
  "coc.source.snippets.priority": 100,
  "yank.highlight.duration": 300,
  "languageserver": {
    "rust": {
      "command": "rust-analyzer",
      "filetypes": ["rust"],
      "rootPatterns": ["Cargo.toml"]
    }
  }
}
